{
    "name": "Svelcro",
    "version": "1.0",
    "minimum_chrome_version": "10.0",
    "manifest_version": 3,
    "devtools_page": "devtools.html",
    "background": {
        "service_worker": "background.js"
    },
    "content_security_policy": {
        "extension pages": "script-src 'self' 'nonce-...' https://d3js.org/d3.v7.min.js; object-src 'self'"},
    "permissions": [ 
        "contextMenus",
        "tabs",
        "activeTab",
        "*://localhost/*"
    ],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["document_start.js"],
        "run_at": "document_start"
    }],
    "web_accessible_resources": [{
        "resources": ["contentScript.js"],
        "matches": ["<all_urls>"]
      }]
    
}
    