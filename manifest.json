{
    "name": "Svelcro",
    "version": "1.0",
    "minimum_chrome_version": "10.0",
    "manifest_version": 2,
    "devtools_page": "devtools.html",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "<all_urls>",
        "contextMenus",
        "tabs",
        "activeTab", 
        "http://*/*", 
        "https://*/*", 
        "file:///*"
    ],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
    }],
    "background": {
        "persistent" : false,
        "scripts" : ["eventPage.js"]
    }
}
    